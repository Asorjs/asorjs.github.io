    "@vue-macros/define-emit": "^0.4.1",
    "@vue-macros/define-models": "1.3.1",
    "@vue-macros/define-prop": "^0.5.1",
    "@vue-macros/define-props": "4.0.1",
    "@vue-macros/define-props-refs": "1.3.1",
